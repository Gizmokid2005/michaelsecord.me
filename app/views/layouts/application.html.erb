<!DOCTYPE html>
<html>
  <head>
    <title>Michael Secord</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>

  <div id="wrap">
    <%= render 'layouts/nav' %>
    <div class="container">
      <%= render 'layouts/header' %>
      <%= yield %>
    </div>
  </div>

  <div id="footer">
    <div class="container center">
      <p class="text-muted"><%= render 'layouts/footer' %></p>
    </div>
  </div>

  </body>
</html>
